programa {
  funcao inicio() {
    inteiro num1, num2, num3

    escreva("Digite o primeiro nÃºmero: ")
    leia(num1)
    escreva("Digite o segundo nÃºmero: ")
    leia(num2)
    escreva("Digite o terceiro nÃºmero: ")
    leia(num3)

    se (num1 >= num2) {
      se (num1 >= num3) {
        escreva(num1, "\n")
        se (num2 >= num3) {
          escreva(num2, "\n")
          escreva(num3, "\n")
        } senao {
          escreva(num3, "\n")
          escreva(num2, "\n")
        }
      } senao {
        escreva(num3, "\n")
        escreva(num1, "\n")
        escreva(num2, "\n")
      }
    } senao {
      se (num2 >= num3) {
        escreva(num2, "\n")
        se (num1 >= num3) {
          escreva(num1, "\n")
          escreva(num3, "\n")
        } senao {
          escreva(num3, "\n")
          escreva(num1, "\n")
        }
      } senao {
        escreva(num3, "\n")
        escreva(num2, "\n")
        escreva(num1, "\n")
      }
    }
  }
}